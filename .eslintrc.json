{
  "settings": {
    "react": {
      "version": "^16.8.6"
    }
  },
   "extends": [
     "eslint:recommended",
     "plugin:react/recommended",
     "plugin:prettier/recommended"
   ],
   "env": {
     "node": true,
     "browser": true,
     "jest": true,
     "es6": true
   },
   "plugins": [
     "react",
     "prettier"
   ],
   "parserOptions": {
     "ecmaVersion": 6,
     "sourceType": "module",
     "ecmaFeatures": {
       "jsx": true
     }
   },
   "rules": {
     "complexity": "error"
   },
   "parser": "babel-eslint"
}